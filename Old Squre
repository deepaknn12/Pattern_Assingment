import java.util.Scanner;
public class Main{
 public static void main(String[] args) {
  Scanner sc=new Scanner(System.in);
  int n=sc.nextInt();
  int i=1;
  int maxodd=(2*n)-1;
  while(i<=n) {
   int j=1;
   int odd=(2*i)-1;
   while(j<=n) {
    if(odd>maxodd) {
     odd=1;
    }
    System.out.print(odd);
    j++;
    odd=odd+2;
   }
   System.out.println();
   i++;
  }  
 }
}
